{
    "compilerOptions": {
        "lib": [
            "ES2019"
        ],
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "strict": true,
        "moduleResolution": "node",
        "types": [
            "node"
        ],
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
    },
}
